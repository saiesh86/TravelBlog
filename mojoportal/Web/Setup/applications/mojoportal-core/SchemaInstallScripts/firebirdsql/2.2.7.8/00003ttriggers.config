CREATE TRIGGER MP_BANNEDIPADDRESSES_BI FOR MP_BANNEDIPADDRESSES ACTIVE
BEFORE INSERT POSITION 0
AS
BEGIN
IF (NEW.RowID  IS NULL) THEN
 NEW.RowID  = NEXT VALUE FOR mp_BannedIPAddresses_seq;
END^
SET TERM ; ^

