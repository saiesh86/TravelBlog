                CREATEDDATE,
                COUNTOFUSEONMYPAGE,
		GUID,
		FEATUREGUID,
                SITEGUID,
                HIDEFROMAUTH,
                HIDEFROMUNAUTH,
                VIEWROLES
) 
VALUES 
(				
               :MODULEID,
               :SITEID,
               :MODULEDEFID,
               :MODULETITLE,
               :AUTHORIZEDEDITROLES,
               :CACHETIME,
               :SHOWTITLE,
               :EDITUSERID,
               :AVAILABLEFORMYPAGE,
               :ALLOWMULTIPLEINSTANCESONMYPAGE,
               :ICON,
               :CREATEDBYUSERID,
               :CREATEDDATE,
               :COUNTOFUSEONMYPAGE,
	      :GUID,
	      :FEATUREGUID,
               :SITEGUID,
               :HIDEFROMAUTH,
               :HIDEFROMUNAUTH,
                :VIEWROLES
);

END^
