SET TERM ^ ;
ALTER PROCEDURE MP_MODULEDEFINITIONS_INSERT (
    FEATURENAME Varchar(255),
    CONTROLSRC Varchar(255),
    SORTORDER Integer,
    ISADMIN Smallint,
    ICON Varchar(255),
    DEFAULTCACHETIME Integer,
    GUID Char(36),
    RESOURCEFILE Varchar(255),
    IsCacheable Smallint,
    IsSearchable Smallint,
    SearchListName Varchar(255) )
RETURNS (
    MODULEDEFID Integer )
AS
BEGIN
 MODULEDEFID = NEXT VALUE FOR mp_ModuleDefinitions_seq;

INSERT INTO 	MP_MODULEDEFINITIONS
(				
                MODULEDEFID,
                FEATURENAME,
                CONTROLSRC,
                SORTORDER,
                ISADMIN,
                ICON,
                DEFAULTCACHETIME,
                GUID,
                IsCacheable,
                IsSearchable,
                SearchListName,
                RESOURCEFILE
) 
VALUES 
(				
               :MODULEDEFID,
               :FEATURENAME,
               :CONTROLSRC,
               :SORTORDER,
               :ISADMIN,
               :ICON,
               :DEFAULTCACHETIME,
               :GUID,
               :IsCacheable,
               :IsSearchable,
               :SearchListName,
               :RESOURCEFILE
);

END^
SET TERM ; ^

